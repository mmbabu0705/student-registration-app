pipeline {
    agent any

    stages {
        stage('Git Clone SCM') {
            steps {
                echo 'This is Clone stage'
            }
        }
        stage('Compile the code') {
            steps {
                echo 'This is Compile stage'
            }
        }
        stage('Test the code') {
            steps {
                echo 'This is Testing stage'
            }
        }
        stage('Package the code') {
            steps {
                echo 'This is package stage'
            }
        }
        stage('Deploy the code') {
            steps {
                echo 'This is Deploy stage'
            }
        }
    }
}
